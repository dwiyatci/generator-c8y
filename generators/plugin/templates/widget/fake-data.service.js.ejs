(function () {
  'use strict';

  angular
    .module('<%= moduleName %>')
    .factory('<%= widgetFakeDataServiceName %>', fakeDataService);

  /* @ngInject */
  function fakeDataService(
    $http,
    c8yBase
  ) {
    const service = {
      getDataForDeviceId
    };

    return service;

    ////////////

    async function getDataForDeviceId(id) {
      const res = await $http.get(':::PLUGIN_PATH:::/fake-data.json');
      const data = await c8yBase.getResData(res);

      return data;
    }
  }
}());
