(function () {
  'use strict';

  angular
    .module('<%= moduleName %>')
    .decorator('<%= recipeName %>', serviceDecorator);

  /* @ngInject */
  function serviceDecorator(
    $delegate
  ) {
    const originalMethod = $delegate.method;

    $delegate.method = decoratedMethod;

    return $delegate;

    ////////////

    function decoratedMethod() {
      const result = originalMethod.apply(this, arguments);

      return result;
    }
  }
}());
