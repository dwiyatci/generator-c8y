(function () {
  'use strict';

  angular
    .module('<%= moduleName %>')
    .decorator('<%= recipeName %>', <%= recipeName %>Decorator);

  /* @ngInject */
  function <%= recipeName %>Decorator(
    $delegate
  ) {
    var originalMethod1 = $delegate.method1;

    $delegate.method1 = decoratedMethod1;

    return $delegate;

    ////////////

    function decoratedMethod1() {
      return originalMethod1.apply(this, arguments);
    }
  }
}());
