.axis path,
.axis line {
  fill: none;
  stroke: black;
  shape-rendering: crispEdges;
}

.axis text {
  font: 10px sans-serif;
}

.<%= widgetCssClassName %> {
  height: calc(100% - 40px);
}

.<%= widgetCssClassName %> > .chart-title {
  font-weight: bold !important;
}

.<%= widgetCssClassName %> > .chart-container {
  height: calc(100% - 56px);
}

