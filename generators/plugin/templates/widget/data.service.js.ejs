(function () {
  'use strict';

  angular
    .module('<%= moduleName %>')
    .factory('<%= widgetDataServiceName %>', dataService);

  /* @ngInject */
  function dataService(
    <%= widgetConstantsName %>,
    <%= widgetFakeDataServiceName %>
  ) {
    const service = {
      getDataForDeviceId
    };

    return service;

    ////////////

    async function getDataForDeviceId(id) {
      const data = await <%= widgetFakeDataServiceName %>.getDataForDeviceId(id);

      return _.transform(data);
    }
  }
}());
