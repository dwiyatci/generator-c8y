(() => {
  'use strict';

  angular.module('<%= moduleName %>').decorator('<%= recipeName %>', serviceDecorator);

  function serviceDecorator($delegate) {
    'ngInject';

    const { method: originalMethod } = $delegate;

    $delegate.method = decoratedMethod;

    return $delegate;

    ////////////

    function decoratedMethod(...args) {
      const result = originalMethod.apply(this, args);

      return result;
    }
  }
})();
