(function () {
  'use strict';

  angular
    .module('<%= moduleName %>')
    .factory('<%= widgetDataServiceName %>', dataService);

  /* @ngInject */
  function dataService(
    <%= widgetConstantsName %>,
    <%= widgetFakeDataServiceName %>
  ) {
    const service = {
      getDataFor
    };

    return service;

    ////////////

    function getDataFor(id) {
      const dataPromise = <%= widgetFakeDataServiceName %>.getDataFor(id);

      return dataPromise
        .then((data) => {
          console.log(data);

          return data;
        });
    }
  }
}());
