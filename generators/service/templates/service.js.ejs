(function () {
  'use strict';

  angular
    .module('<%= moduleName %>')
    .factory('<%= serviceName %>', <%= serviceName %>);

  /* @ngInject */
  function <%= serviceName %>() {
    var service = {
      method: method
    };

    return service;

    ////////////

    function method() {
      return 'hello, world';
    }
  }
}());
